{
    "name": "codeliner/php-ddd-cargo-sample",
    "description": "PHP DDD Cargo Sample",
    "license": "BSD-3-Clause",
    "keywords": [
        "DDD",
        "cargo sample"
    ],
    "homepage": "https://github.com/codeliner/php-ddd-cargo-sample",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "kontakt@codeliner.ws",
            "homepage": "http://www.codeliner.ws/"
        }
    ],
    "require": {
        "php": ">=5.5",
        "codeliner/php-equalsbuilder": "~1.1",
        "codeliner/shared-domain-set": "1.0",
        "ramsey/uuid" : "~2.8",
        "zendframework/zend-expressive": "*@dev",
        "zendframework/zend-servicemanager": "^2.6",
        "aura/router": "^2.3",
        "zendframework/zend-stdlib": "^2.5",
        "doctrine/orm" : "^2.5"
    },
    "require-dev" : {
        "phpunit/phpunit": "~4.7",
        "behat/behat": "~2.4",
        "behat/mink": "~1.4",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload" : {
        "psr-4" : {
            "Codeliner\\CargoBackend\\"  : "CargoBackend/src",
            "Codeliner\\CargoUI\\" : "CargoUI/src",
            "Codeliner\\GraphTraversalService\\" : "GraphTraversalService/src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "CodelinerTest\\CargoBackend\\"  : "CargoBackend/tests",
            "CodelinerTest\\GraphTraversalService\\" : "GraphTraversalService/tests"
        }
    }
}
